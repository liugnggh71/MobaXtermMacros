<?xml version="1.0" encoding="UTF-8"?>
<macro xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../MobaMacro.xsd">
   <name>PWD_get_random_password</name>
   <desc>Full code file: PWD_get_random_password.sql</desc>
   <environment>SQLPlus</environment>
   <hotkey>0</hotkey>
   <line type="Text">COLUMN passwd NEW_VALUE passwd</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">SELECT DBSNMP.GenPassword (9, 3, 2) passwd FROM DUAL;</line>
   <line type="KeyPress">RETURN</line>
</macro>
