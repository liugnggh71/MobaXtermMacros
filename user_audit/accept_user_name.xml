<?xml version="1.0" encoding="UTF-8"?>
<macro xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../MobaMacro.xsd">
   <name>accept_user_name</name>
   <desc>Full code file: accept_user_name.sql</desc>
   <environment>SQLPlus</environment>
   <hotkey>0</hotkey>
   <line type="Text">SELECT username, created FROM dba_users ORDER BY 1;</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text"> </line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">accept username char prompt 'Enter New Username: '</line>
   <line type="KeyPress">RETURN</line>
</macro>
