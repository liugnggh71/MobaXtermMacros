<?xml version="1.0" encoding="UTF-8"?>

<macro xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../MobaMacro.xsd">
    <name>WINDOWS host uptime</name>
    <desc>Find out when windows host is brought up</desc>
    <environment>cmd</environment>
    <hotkey>0</hotkey>
    <line type="Text">net stats srv</line>
    <line type="KeyPress">RETURN</line>
</macro>
