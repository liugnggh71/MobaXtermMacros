<?xml version="1.0" encoding="UTF-8"?>

<macro xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../MobaMacro.xsd">
    <name>conn dbinv</name>
    <desc>Get into inventory database</desc>
    <environment>SQLPlus</environment>
    <hotkey>0</hotkey>
    <line type="Text">conn dbinv/Ora_cle1@cmparch:1521/dbadmin</line>
    <line type="KeyPress">RETURN</line>
    <line type="Text">set sqlprompt "_user '@' _connect_identifier > "</line>
    <line type="KeyPress">RETURN</line>
</macro>
