<?xml version="1.0" encoding="UTF-8"?>
<macro xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../MobaMacro.xsd">
   <name>CONFIG_DDBoost_pwd</name>
   <desc>Full code file: CONFIG_DDBoost_pwd.ksh</desc>
   <environment>KornShell</environment>
   <hotkey>0</hotkey>
   <line type="Text">rman &lt;&lt; EOF</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">connect target</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">RUN {</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">  allocate channel C1 device type SBT_TAPE parms</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">     'SBT_LIBRARY=${ORACLE_HOME}/lib/libddobk.so,</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">      SBT_PARMS=(ORACLE_HOME=${ORACLE_HOME})';</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">  send 'set username ddboostninja password baylor01 servername BHDAMSDD02.BHCS.PVT';</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">  send 'set username ddboostninja password baylor01 servername BHDASVDD02.BHCS.PVT';</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">  RELEASE CHANNEL C1;</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">  backup current controlfile;</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">}</line>
   <line type="KeyPress">RETURN</line>
   <line type="Text">EOF</line>
   <line type="KeyPress">RETURN</line>
</macro>
